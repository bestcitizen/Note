---
layout: none
---
[
  {% for skill in site.skills %}
    {
      "title": "{{ skill.title | escape }}",
      "url": "{{ site.baseurl }}{{ skill.url }}",
      "date": "{{ skill.date }}",
      "content": {{ skill.content | jsonify }}
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]